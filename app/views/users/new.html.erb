<%= render partial: "layouts/error_message", locals: {thing: @user} %>
<h2> Create a New User </h2>

<%= form_for @user do |f| %>
  <%= f.label "Email" %>
  <%= f.text_field :email %><br>

  <%= f.label "Password" %>
  <%= f.text_field :password %><br>

  <%= f.label "Access Code" %>
  <%= f.text_field :admin_level %><br>

  <%= f.submit "Submit" %><br>
<% end %>
